{
    "name": "插件",
    "manifest_version": 2,
    "description": "create chrome extend plugin",
    "version": "1.0",
    "icons": {
        "16": "icon/app.png"
    },
    "scriptsAndCss": [
        {
            "js": [
                "./inject.js"
            ],
            "position": "document_start"
        }
    ],
    "background": {
        "scripts": [
            "./background.js"
        ]
    },
    "browser_action": {
        "default_icon": "icon/app.png",
        "default_title": "chajian",
        "default_popup": "popup.html"
    }
}