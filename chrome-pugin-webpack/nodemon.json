{
    "restartable": "rs",
    "ignore": [
      ".git",
      "node_modules/**/node_modules"
    ],
    "verbose": true,
    "execMap": {
      "js": "node --harmony"
    },
    "watch": [
      "build/",
      "src/",
      "manifest.json",
      "webpack.config.js"
    ],
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "js,json"
  }