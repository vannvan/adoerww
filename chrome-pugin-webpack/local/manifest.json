{
    "name": "插件",
    "manifest_version": 2,
    "description": "webpack版chrome插件",
    "version": "1.0",
    "permissions": [
        "contextMenus",
        "tabs",
        "notifications",
        "webRequest",
        "webRequestBlocking",
        "storage",
        "http://*/*",
        "https://*/*"
    ],
    "options_ui": {
        "page": "options.html",
        "chrome_style": true
    },
    "homepage_url": "https://github.com/vannvan",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "inject.js"
            ],
            "run_at": "document_end"
        }
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
        "default_icon": "icon/app.png",
        "default_title": "插件",
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "icon/app.png",
        "48": "icon/app.png",
        "128": "icon/app.png"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}