{
    "manifest_version": 2,
    "name": "vannvan",
    "version": "1.0.0",
    "description": "开发者工具",
    "author": "vannvan",
    "icons": {
        "48": "icon.png",
        "128": "icon.png"
    },
    "homepage_url": "https://github.com/vannvan",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "contextMenus",
        "tabs",
        "notifications",
        "webRequest",
        "webRequestBlocking",
        "storage",
        "http://*/*",
        "https://*/*"
    ],
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["./js/content-script.js", "./js/baidu-style.js"],
        "css": ["./css/content-style.css"],
        "run_at": "document_end"
    }],
    "background": {
        "page": "background.html"
    },
    "web_accessible_resources": ["inject.js"],

    "devtools_page": "devtools.html"
}